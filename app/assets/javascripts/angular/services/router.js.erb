'use strict'

rclvapp.config(['$routeProvider',
  function($routeProvider){
    $routeProvider.
    when("/", {
      templateUrl: "<%=asset_path 'angular/templates/home.html' %>",
      controller: "homeContr"
    }).
    when('/news_items', {
      templateUrl: "<%=asset_path 'angular/templates/news_items.html' %>",
      controller: 'newsListContr'
    })
  }
])